    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "3.16.1",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.2.5",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
