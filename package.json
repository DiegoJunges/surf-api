  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js | pino-pretty -c",
    "dev:server": "ts-node-dev --transpile-only --ignore-watch node_modules 'src/index.ts' | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "start:local": "ts-node-dev 'src/index.ts'",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "test:all": "yarn test:unit && yarn test:functional"
  },
  "author": "Diego Junges <diegojunges@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.168",
    "@types/module-alias": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.10",
    "@types/pino": "^6.3.5",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.2.0",
    "jest": "^26.6.3",
    "nock": "^13.0.7",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
